{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Alpha",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/dgraph/",
            "env": {},
            "args": ["alpha", "--telemetry", "sentry=false;", "--security", "whitelist=0.0.0.0/0;"],
            "showLog": true
        },
        {
            "name": "Alpha-0",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/dgraph/",
            "env": {},
            "args": ["alpha", "--telemetry", "sentry=false;", "-o", "104", "--my", "localhost:7184", "--zero", "localhost:5080",
        "--raft", "snapshot-after-duration=0;snapshot-after-entries=11;", "-v", "2"],
            "showLog": true
        },
        {
            "name": "Zero",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/dgraph/",
            "env": {},
            "args": ["zero", "--telemetry", "sentry=false;"],
            "showLog": true
        },
        {
            "name": "AlphaACL",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/dgraph/", 
            "env": {},
            "args": ["alpha", "--trace", "jaeger=http://localhost:14268; ratio=0.99;", "--telemetry", "sentry=false;", "--security", "whitelist=0.0.0.0/0;", "--acl", "secret-file=../ee/acl/hmac-secret; access-ttl=300000s;"],
            "showLog": true
        },
        {
            "name": "Launch test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${fileDirname}",
            "args": ["-test.run", "MyTestFunction"],
            "env": {
                "TEST_DOCKER_PREFIX": "snap"
            },
            "buildFlags": ["-v", "tags=integration iunit_test"]
        }
    ],
    "compounds": [
        {
            "name": "normal",
            "configurations": ["Alpha", "Zero"]
        },
        {
            "name": "ACL",
            "configurations": ["AlphaACL", "Zero"]
        }
    ]
}
