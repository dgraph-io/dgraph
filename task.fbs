namespace task;

table Query {
	attr:string;
	uids:[ulong];
	count:int;
	offset:int;
	afterUid:ulong;
	getCount:ushort;
	terms:[string];
}

table Value {
	val:[ubyte];
	valType:ubyte;
}

table XidList {
	xids:[string];
}

table UidList {
	uids:[ulong];
}

table Result {
	uidmatrix:[UidList];
	values:[Value];
	count:[ulong];
}

table KV {
	key:[ubyte];
	val:[ubyte];
}

table KC {
	key:[ubyte];
	checksum:[ubyte];
}

table GroupKeys {
	groupid:ulong;
	keys:[KC];
}
